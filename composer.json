{
    "name":     "conductor/conductor",
    "type":     "composer-installer",
    "description": "Composer installer-related tools",
    "license":  "MIT",
    "keywords": ["composer", "scripts", "installer"],
    "homepage": "https://github.com/claylo/conductor",
    "authors": [
        {
            "name": "Clay Loveless",
            "email": "clay@php.net",
            "homepage": "http://claylo.com/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/filesystem": "2.1.*"
    },
    "suggest": {
        "symfony/framework-standard-edition": "Required to make use of the Conductor\\Tasks\\BootstrapSymfonyStandardEdition task."
    },
    "support": {
        "issues": "https://github.com/claylo/conductor/issues",
        "twitter": "@claylo"
    },
    "autoload": {
        "psr-0": {
            "Conductor": ""
        }
    },
    "extra": {
        "class": "Conductor\\ScriptInstaller",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "package2composer.php"
    ]
}